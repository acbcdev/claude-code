{
  "name": "acbc-code",
  "owner": {
    "name": "acbc",
    "email": "acbc.dev@gmail.com",
    "url": "https://acbc.dev"
  },
  "metadata": {
    "description": "A collection of plugins for Claude",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "git-master",
      "description": "Git plugin For managing git repositories Changelog",
      "version": "1.0.1",
      "keywords": ["git", "changelog", "version", "release"],
      "license": "MIT",
      "author": { "name": "acbcdev" },
      "category": "git",
      "strict": false,

      "repository": "https://github.com/acbcdev/claude-code",
      "source": {
        "source": "github",
        "repo": "acbcdev/claude-code"
      },
      "agents": [
        "./git-master/agents/commit-generator.md",
        "./git-master/agents/git-branch-manager.md",
        "./git-master/agents/changelog-manager.md"
      ],
      "commands": ["./git-master/commands/commit.md"],
      "skills": ["./git-master/skills/commiter.md"]
    }
  ]
}
